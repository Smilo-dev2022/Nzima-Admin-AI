  "keywords": [
    "admin",
    "ai",
    "business-management",
    "sme",
    "african",
    "multi-platform",
    "react",
    "express",
    "firebase"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "notistack": "^3.0.2",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.0",
    "typescript": "^5.5.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.0"
  },
  "babel": {
    "presets": ["@babel/preset-env", "@babel/preset-react"]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"]
  },
  "eslintConfig": {
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
