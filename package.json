{
  "name": "nzima-admin-ai",
  "version": "1.0.0",
  "description": "Nzima-Admin-AI is a multi-platform admin and business management solution for African SMEs, powered by AI, with support for web, mobile, and backend integrations.",
  "main": "index.js",
  "author": "Nzima Admin AI Team <team@nzima-admin.com>",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Smilo-dev2022/Nzima-Admin-AI.git"
  },
  "homepage": "https://github.com/Smilo-dev2022/Nzima-Admin-AI#readme",
  "bugs": {
    "url": "https://github.com/Smilo-dev2022/Nzima-Admin-AI/issues"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "echo \"Build process - configure as needed for your project\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
